<local:BasePage x:TypeArguments="core:ManageViewModel" x:Class="NetworkEngine.Manager.Pages.ManagePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NetworkEngine.Manager.Pages"
      xmlns:controls="clr-namespace:NetworkEngine.Manager.Controls"
      xmlns:core="clr-namespace:NetworkEngine.Manager.Core.ViewModels;assembly=NetworkEngine.Manager.Core"
      xmlns:design="clr-namespace:NetworkEngine.Manager.Core.ViewModels.Design;assembly=NetworkEngine.Manager.Core"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Manage Page">

    <Grid Margin="5">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="250"/>
            <ColumnDefinition Width="Auto"/>
        </Grid.ColumnDefinitions>

        <DockPanel>
            <controls:NodeStructureUserControl DataContext="{Binding NodeStructure}"/>
        </DockPanel>
    </Grid>
</local:BasePage>
